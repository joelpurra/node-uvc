{
  "name": "uvc",
  "version": "0.0.0",
  "description": "Library for USB Video Class (UVC) devices. Used to write software for webcams, camcorders, etcetera.",
  "homepage": "https://joelpurra.com/projects/node-uvc/",
  "main": "index.js",
  "scripts": {
    "test": "npm run --silent lint:fast",
    "lint": "npm run --silent lint:full",
    "lint:fast": "npm run --silent prettier && npm run --silent lint:copyright",
    "lint:full": "npm run --silent lint:fast",
    "lint:fix": "npm run --silent prettier:fix",
    "lint:copyright": "find . -not \\( -path './.git/*' -or -path './node_modules/*' \\) -type f \\( -iname '*.js' -and -not \\( -path './.huskyrc.js' -or -path './lib/libuvc.js' -or -path './test/stream-disk/stream-disk.js' \\) \\) -print0 | xargs -0 grep -L 'This file is part of node-uvc' | sed 's/^/File is missing copyright notice: /'",
    "prettier": "prettier --list-different './**/*.js' './**/*.json' './**/*.md'",
    "prettier:fix": "prettier --write './**/*.js' './**/*.json' './**/*.md'"
  },
  "dependencies": {},
  "devDependencies": {
    "ffi-napi": "github:atishay/node-ffi-napi#40443ca8dad10cc3286c432774de3ce631d457e1",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "ref-array-di": "^1.2.1",
    "ref-napi": "^1.4.3",
    "ref-struct-di": "^1.1.0",
    "ref-union-di": "^1.0.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "usb",
    "usb video class",
    "uvc",
    "camera",
    "webcamera",
    "video",
    "photo",
    "capture",
    "controls",
    "configuration",
    "libusb",
    "libuvc"
  ],
  "bugs": {
    "url": "https://github.com/joelpurra/node-uvc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelpurra/node-uvc.git"
  },
  "author": {
    "name": "Joel Purra",
    "email": "mig@joelpurra.se",
    "url": "https://joelpurra.com/"
  },
  "license": "LGPL-3.0"
}
